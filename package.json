  "license": "MIT",
  "description": "bloke theme",
  "homepage": "https://github.com/DavidKk/bloke-theme-sharp",
  "author": "DavidJones",
  "main": "./dist/bloke.config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidKk/bloke-theme-sharp.git"
  },
  "engines": {
    "node": ">= 6.2.2"
  },
  "scripts": {
    "compile": "babel ./src/bloke.config.js --out-file ./dist/bloke.config.js",
    "webpack:develop": "webpack --config ./webpack/webpack.develop.config.babel.js --watch",
    "webpack:release": "webpack --progress --profile --hide-modules --config ./webpack/webpack.product.config.babel.js",
    "develop": "npm run webpack:develop",
    "release": "npm run webpack:release && npm run compile",
    "start": "npm run develop"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "fs-extra": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-pug-plugin": "0.0.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
